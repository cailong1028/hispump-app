        <div class="panel-heading">
            <div class="row">
                <div class="col-lg-9 col-md-9">
                    <h3>
                        <span class="pull-right">
                            <a class="btn btn-default" href="javascript: window.history.back();" data-bypass>{{gettext 'Back'}}</a>
                            <button id="add-dev" class="btn btn-default">{{gettext 'Add dev'}}</button>
                        </span>
                        {{gettext 'Dept dev'}}
                    </h3>
                </div>
                <div class="col-lg-3 col-md-3">
                    <h5>
                        {{gettext 'Dept dev title'}}
                    </h5>
                </div>
            </div>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-lg-9 col-md-9">
                    <div class="tab-content">
                        <div class="tab-pane fade in active" role="tabpanel">
                            <div class="clearfix"></div>
                            <div class="list"></div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 text-muted">{{{gettext 'Dept dev help document'}}}</div>
