<div class="container tickets-add">
    <div class="panel panel-content">
        <div class="panel-heading">
            <div class="row">
                <div class="col-md-9 col-lg-9">
                    <h3>
                        {{gettext 'Commit Ticket'}}
                    </h3>
                </div>
                <div class="col-md-3 col-lg-3">
                    <h5>
                        {{gettext 'Add ticket title'}}
                    </h5>
                </div>
            </div>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-md-9 col-lg-9">
                    <form class="ticket-form form-horizontal">
                        <div class="form-group">
                            <div class="col-md-12 co-lg-12">
                                <label class="required">{{gettext 'Search Requester'}} {{gettext 'or'}} <b><a class="addNewContacts" href="#add-new-contacts" data-bypass>{{gettext 'add new requester'}}</a></b></label>
                                <br>
                                <input id="requester" class="group-select form-control" name="requester" type="text">
                                <span id="requesterNameInvalid" class="error-messages text-danger">{{gettext "Requester Request"}}</span>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-12 co-lg-12">
                                <label class="required">{{gettext "Subject"}}</label>
                                <br>
                                <input class="form-control" name="subject" maxlength="128" placeholder="{{gettext 'Subject'}}" type="text">
                                <span id="subjectCanNotBeNull" class="error-messages text-danger">{{gettext "Subject Can Not Be Null"}}</span>
                                <span id="subjectTooLang" class="error-messages text-danger">{{gettext "Too Many Words"}}</span>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-5 col-lg-5">
                                <label>{{gettext "Type"}}</label>
                                <br>
                                <select name="type" class="tickets-type form-control">
                                    {{#each type}}
                                    <option value="{{this}}">{{gettext this}}</option>
                                    {{/each}}
                               </select>
                            </div>
                            <div class="col-md-5 col-lg-5 col-lg-offset-2 col-md-offset-2">
                                <label>{{gettext "Ticket State"}}</label>
                                <br>
                                <select name="state" class="form-control">
                                    {{#each ticketState}}
                                    <option value="{{agentDisplay}}">{{gettext agentDisplay}}</option>
                                    {{/each}}
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-5 col-lg-5">
                                <label>{{gettext "Priority"}}</label>
                                <br>
                                <select name="priority" class="tickets-priority form-control">
                                    <option value="20">{{gettext 'Priority 20'}}</option>
                                    <option value="40">{{gettext 'Priority 40'}}</option>
                                    <option value="60">{{gettext 'Priority 60'}}</option>
                                    <option value="80">{{gettext 'Priority 80'}}</option>
                                </select>
                            </div>
                            <div class="col-md-5 col-lg-5 col-md-offset-2 col-lg-offset-2">
                                <label>{{gettext "Group"}}</label>
                                <br>
                                <select name="group" class="tickets-group form-control">
                                    <option value="">--</option>
                                    {{#each groups}}
                                    <option value="{{id}}">{{name}}</option>
                                    {{/each}}
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-5 col-lg-5">
                                <label>{{gettext "Assignee Agent"}}</label>
                                <select name="assignee" class="assignee form-control">
                                    <option value="">--</option>
                                    {{#each agents}}
                                    <option value="{{username}}" {{#if_eq ../currAgent.username username}}
                                        selected="selected"
                                    {{/if_eq}}>{{name}}</option>
                                    {{/each}}
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-12 col-lg-12">
                                <label>{{gettext "Description"}}</label>
                                <br>
                                <textarea id="ticket-description" name="description" row="10" class="form-control" placeholder="{{gettext 'Description'}}"></textarea>
                                <span id="DescriptionSizeExceedsTheAllowableLimit" class="error-messages text-danger">{{gettext "Description length exceeds the allowable limit"}}</span>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-12 col-lg-12">
                                <div id="file-uploader"></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-12 col-lg-12">
                                <span class="help-text">{{gettext 'Upload file title'}}</span>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-12 col-lg-12 text-right">
                                <button class="cancel btn btn-default">{{gettext "Cancel"}}</button>
                                <span class="btn-group dropup submits">
                                    <button type="submit" class="btn btn-primary">{{gettext "Save"}}</button>
                                    <button class="btn btn-primary dropdown-toggle" data-toggle="dropdown">
                                        <span class="caret"></span>
                                    </button>
                                    <ul class="dropdown-menu dropdown-menu-right" role="menu">
                                        <li><a class="save-close" href="#save-close"  data-bypass>{{gettext "Save And Close"}}</a></li>
                                        <li class="divider"></li>
                                        <li><a class="save-continue" href="#save-continue" data-bypass>{{gettext "Save And Continue"}}</a></li>
                                    </ul>
                                </span>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="col-lg-3 col-md-3 text-muted">{{{gettext 'Add ticket help document'}}}</div>
            </div>
        </div>
    </div>
</div>
<div class="add-new-requester"></div>
