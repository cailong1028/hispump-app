<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th class="pick"><p><input type="checkbox" class="picker"></p></th>
            <th colspan="5">
                <button class="btn btn-default btn-xs assign-to-me" disabled="disabled">{{gettext 'Assign to me'}}</button>
                <button class="btn btn-default btn-xs assign-to" disabled="disabled">{{gettext 'Assign'}}</button>
                <button class="btn btn-default btn-xs close-tickets" disabled="disabled">{{gettext 'Close'}}</button>
                {{#isPermitted 'tickets:delete'}}<button class="btn btn-default btn-xs delete-tickets" disabled="disabled">{{gettext 'Delete'}}</button>{{/isPermitted}}
            </th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <td colspan="6"></td>
        </tr>
    </tfoot>
    <tbody>
        <tr class="loading">
            <td colspan="6"><p>{{gettext 'Now Loading...'}}</p></td>
        </tr>
        <tr class="non-items">
            <td colspan="6"><p>{{gettext 'Non Tickets Items'}}</p></td>
        </tr>
    </tbody>
</table>
<div>
    <ul id="pagination" class="pagination pagination-sm"></ul>
</div>
<form id="assginForm">
<div class="assign-to-model modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">{{gettext 'Assign Tickets to Agent'}}</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <input id="choose-assignee" class="choose-assignee form-control">
                    <span class="hide-msg text-danger hide">{{gettext "No Agent"}}</span>
                    <span id="agentRequired" class="text-danger error-messages form-group">{{gettext "Agent is not allowed to be empty"}}</span>
                </div>
            </div>
            <div class="modal-footer">
                <button class="cancel btn btn-default">{{gettext "Cancel"}}</button>
                <button class="assign btn btn-primary">{{gettext "Assign"}}</button>
            </div>
        </div>
    </div>
</div>
</form>
