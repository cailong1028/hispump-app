<table class="table table-striped table-bordered corporations">
    <thead>
        <tr>
            <th class="pick">{{#isPermitted 'corporations:delete'}}<input type="checkbox" class="picker">{{/isPermitted}}</th>
            <th colspan="2">
                {{#isPermitted 'corporations:delete'}}
                <a class="batchDelete btn btn-default btn-xs" disabled>{{gettext "Delete"}}</a>
                {{/isPermitted}}
            </th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <td colspan="3"></td>
        </tr>
    </tfoot>
    <tbody>
    <tr class="loading"><td colspan="3"><p>{{gettext 'Now Loading...'}}</p></td></tr>
    <tr class="non-items"><td colspan="3"><p>{{gettext 'Non Corporations'}}</p></td></tr>
    </tbody>
</table>
<ul id="pagination" class="pagination pagination-sm"></ul>

